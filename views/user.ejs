<%-include header%>
    <%if(user){%>
        <% articles.forEach(function(article){ %>
            <p>
                <h2>
                    <a href="/user/<%= article.name%>/<%= article.time.day%>/<%= article.title%>">
                        <%= article.title%>
                    </a>
                </h2>
            </p>
            <p class="info">
                作者：
                <a href="/user/<%= article.name%>">
                    <%= article.name%>
                </a>
                日期：
                <%= article.time.minute%>
            </p>
            <p>
                <%- article.content%>
            </p>
        <%})%>
    <%}%>
<%-include paging%>
<%-include footer%>