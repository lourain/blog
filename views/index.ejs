<%-include header%>
  <%if(user){%>
    <% articles.forEach(function(article){ %>
      <p>
        <h2>
          <a href="/user/<%= article.name%>/<%= article.time.day%>/<%= article.title%>">
            <%= article.title%>
          </a>
        </h2>
      </p>
      <p class="info">
        作者：
        <a href="/user/<%= article.name%>">
          <%= article.name%>
        </a>
        日期：
        <%= article.time.minute%>
         标签：
        <% article.tags.forEach(function(tag){ %>
          <%if(tag){%>
            <a class="tag" href="/tags/<%= tag %>"><%=tag%></a>
          <%}%>
        <% }) %>
      </p>
      <p>
        <%- article.content%>
        <p class=""></p>
      </p>
      <%})%>
  <%}%>
<%-include paging%>
<%-include footer%>